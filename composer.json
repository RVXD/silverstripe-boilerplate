{
  "name": "bramdeleeuw/recipe-boilerplate",
  "type": "silverstripe-recipe",
  "description": "Boilerplate template to quickly kick off a new SilverStripe project",
  "require": {
    "php": ">=5.6.0",
    "silverstripe/recipe-cms": "1.0.0@stable",
    "silverstripe/googlesitemaps": "*",
    "silverstripe/userforms": "*",
    "silverstripe/redirectedurls": "*",
    "silverstripe/spamprotection": "*",
    "silverstripe/html5": "*",
    "symbiote/silverstripe-gridfieldextensions": "*",
    "unclecheese/betterbuttons": "*",
    "colymba/gridfield-bulk-editing-tools": "*",
    "mobiledetect/mobiledetectlib": "*",
    "heyday/silverstripe-versioneddataobjects": "*",
    "heyday/silverstripe-cacheinclude": "*",
    "jonom/silverstripe-betternavigator": "*",
    "littlegiant/silverstripe-catalogmanager": "*",
    "littlegiant/silverstripe-singlepageadmin": "*",
    "tractorcow/silverstripe-opengraph": "*",
    "undefinedoffset/silverstripe-nocaptcha": "*",
    "gdmedia/ss-auto-git-ignore": "*",
    "bramdeleeuw/silverstripe-schema": "*",
    "bramdeleeuw/silverstripe-pageslices": "*",
    "bramdeleeuw/silverstripe-pageslices-userform": "dev-master"
  },
  "require-dev": {
    "phpunit/PHPUnit": "^5.7"
  },
  "extra": {
    "project-files": [
      "mysite/*",
      ".gitignore",
      ".htaccess",
      "favicon.ico",
      "favicon-144.png",
      "favicon-152.png",
      "robots.txt",
      "web.config"
    ]
  },
  "scripts": {
    "post-update-cmd": "GDM\\SSAutoGitIgnore\\UpdateScript::Go"
  },
  "config": {
    "process-timeout": 600
  },
  "minimum-stability": "dev",
  "prefer-stable": true,

  "bak": {
    "jonom/silverstripe-tinytidy": "*",
    "silverstripe/secureassets": "*",
    "lrc/silverstripe-link-field": "*",
    "littlegiant/silverstripe-seo-editor": "*",
    "markguinn/silverstripe-email-helpers": "*",
    "jonom/focuspoint": "*",
    "jonom/silverstripe-share-care": "*",
    "jonom/silverstripe-text-target-length": "*",
    "unclecheese/display-logic": "*",
    "unisolutions/silverstripe-copybutton": "*",
    "symbiote/silverstripe-addressable": "*",
    "symbiote/silverstripe-grouped-cms-menu": "*",
    "hubertusanton/silverstripe-seo": "*"
  }
}
